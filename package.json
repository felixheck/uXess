{
  "name": "uxess",
  "version": "0.0.1",
  "description": "Client-side authorization middleware to handle ui element based on permissions",
  "main": "./src/uxess.js",
  "scripts": {
    "start": "npm run watch",
    "test": "karma start ./config/karma.conf.js",
    "test:e2e": "npm run protractor --force",
    "build": "npm run build:dist && npm run build:cache",
    "build:dist": "node config/buildify.conf.js dist:build",
    "build:cache": "npm-html2js -i 'src/partials/**/*.tpl.html' -o 'src/templates.js' -m 'templates' -b 'src/partials'",
    "watch": "gacher -p 'src/**/*.js' -c 'npm run build:dist'",
    "cover": "opn test/coverage/index.html --no-wait -- 'google chrome'",
    "protractor": "node_modules/.bin/protractor config/protractor.conf.js",
    "selenium": "node_modules/.bin/webdriver-manager start"
  },
  "keywords": [],
  "author": {
    "name": "Felix Heck",
    "email": "hi@whoTheHeck.de",
    "url": "http://whoTheHeck.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixheck/uXess.git"
  },
  "bugs": {
    "url": "https://github.com/felixheck/uXess/issues",
    "email": "hi@whoTheHeck.com"
  },
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "^1.4.7",
    "buildify": "^0.4.0",
    "gacher": "^1.1.3",
    "istanbul": "^0.4.0",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.11",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "npm-html2js": "^0.1.8",
    "opn-cli": "^1.0.0",
    "phantomjs": "^1.9.18",
    "protractor": "^2.5.1"
  },
  "dependencies": {},
  "engines": {
    "node": "0.12.7",
    "npm": "^2.11.3"
  }
}
